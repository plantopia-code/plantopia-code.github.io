<xml xmlns="http://www.w3.org/1999/xhtml">
  <variables></variables>
  <block type="device_forever" x="0" y="3">
    <statement name="HANDLER">
      <block type="sampleTurn">
        <field name="direction">Direction.Left</field>
        <value name="angle">
          <shadow type="math_number_minmax">
            <mutation min="-180" max="180" label="Angle"></mutation>
            <field name="SLIDER">90</field>
          </shadow>
        </value>
      </block>
    </statement>
  </block>
  <block type="onBump" x="331" y="0"></block>
  <block type="sampleForward" disabled="true" x="64" y="51">
    <value name="steps">
      <shadow type="math_number">
        <field name="NUM">1</field>
      </shadow>
    </value>
  </block>
</xml>