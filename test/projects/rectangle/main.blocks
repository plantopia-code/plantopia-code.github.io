<xml xmlns="http://www.w3.org/1999/xhtml">
  <variables></variables>
  <block type="device_forever" x="20" y="20">
    <statement name="HANDLER">
      <block type="controls_repeat_ext">
        <value name="TIMES">
          <shadow type="math_whole_number">
            <field name="NUM">4</field>
          </shadow>
        </value>
        <statement name="DO">
          <block type="sampleForward">
            <value name="steps">
              <shadow type="math_number">
                <field name="NUM">1</field>
              </shadow>
            </value>
            <next>
              <block type="sampleTurn">
                <field name="direction">Direction.Left</field>
                <value name="angle">
                  <shadow type="math_number_minmax">
                    <mutation min="-180" max="180" label="Number"></mutation>
                    <field name="SLIDER">90</field>
                  </shadow>
                </value>
              </block>
            </next>
          </block>
        </statement>
      </block>
    </statement>
  </block>
</xml>